{
  "name": "rescript-next-react-tailwind",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".mjs",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}